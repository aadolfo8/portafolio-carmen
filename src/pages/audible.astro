<Layout
  title="Audible"
  description="Encontrar una postura cómoda para leer a veces es muy complicado"
>
   <main
    class="main-project"
    transition:name={`image-audible`}
    transition:animate={fade({ duration: "0.6s" })}
  >
    <section class="image-section">
      <img
        src={cabecera.src}
        alt={cabecera.alt}
        class="full-width-image"
        loading="lazy"
      />
    </section>

    <section class="image-section">
      <img
        src={racional.src}
        alt={racional.alt}
        class="full-width-image"
        loading="lazy"
      />
