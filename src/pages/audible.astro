    <section class="image-section">
      <img
        src={graficas.src}
        alt={graficas.alt}
        class="full-width-image"
        loading="lazy"
      />
    </section>

    <section class="image-section">
      <img
        src={metro.src}
        alt={metro.alt}
        class="full-width-image"
        loading="lazy"
      />
    </section>

    <section class="image-section">
      <img
        src={graficas2.src}
        alt={graficas2.alt}
        class="full-width-image"
        loading="lazy"
      />
